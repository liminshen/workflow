<!doctype html>
<html lang="zh-cn">
    <head>
        <meta charset="UTF-8">
        <title>{{title}}</title>
        <meta name="keywords" content="{{keywords}}">
        <meta name="description" content="{{description}}">
        <meta name="render" content="webkit|ie-comp|ie-stand">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <link rel="stylesheet" href="assets/css/{{cssFile}}?v={{moment unixtimestamp  format ="YYYYMMDDHHmmss"}}">
    </head>
    <body>
        {% body %}
        {{#each cdnURLs}}
            <script src="{{.}}"></script>
        {{/each}}
        {{#if vendors}}
            <script src="assets/js/{{vendorsjsName}}?v={{moment unixtimestamp  format ="YYYYMMDDHHmmss"}}"></script>
        {{/if}}
        {{#if useCommonChunk}}
            <script src="assets/js/{{commonjsName}}?v={{moment unixtimestamp  format ="YYYYMMDDHHmmss"}}"></script>
        {{/if}}
        <script src="assets/js/{{jsFile}}?v={{moment unixtimestamp  format ="YYYYMMDDHHmmss"}}"></script>
    </body>
</html>
