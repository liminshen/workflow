<!doctype html>
<html lang="zh-cn">
    <head>
        <title>{{title}}</title>
        <meta charset="UTF-8">
        <meta name="keywords" content="{{keywords}}">
        <meta name="description" content="{{description}}">
        <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
        <meta name="format-detection" content="telephone=no,email=no">
        <meta name="apple-mobile-web-app-capable" content="no">
        <meta name="apple-touch-fullscreen" content="yes">
        <meta name="screen-orientation" content="portrait">
        <meta name="x5-orientation" content="portrait">
        {{> fixFontSize}}
        {{#each cssCdnURLs}}
        <link rel="stylesheet" href="{{.}}">
        {{/each}}
        {{#each cssLocalCdnURLs}}
        <link rel="stylesheet" href="{{@root.cdnHost}}{{.}}">
        {{/each}}
        {{#if cssFile}}
        <link rel="stylesheet" href="{{relateToAssets}}css/{{cssFile}}?v={{moment unixtimestamp  format ="YYYYMMDDHHmmss"}}">
        {{/if}}
    </head>
    <body>
        {% body %}
        <script type="text/javascript" src="{{cdnHost}}/js/jQuery/jquery-3.1.1.min.js"></script>
        <script type="text/javascript" src="{{relateToAssets}}js/iconfont/iconfont.js"></script>
        <script type="text/javascript" src="{{cdnHost}}/js/util.js"></script>
        {{#each cdnURLs}}
        <script src="{{.}}"></script>
        {{/each}}
        {{#each localCdnURLs}}
        <script src="{{@root.cdnHost}}{{.}}"></script>
        {{/each}}
        {{#if vendors}}
        <script src="{{relateToAssets}}js/{{vendorsjsName}}?v={{moment unixtimestamp  format ="YYYYMMDDHHmmss"}}"></script>
        {{/if}}
        {{#if useCommonChunk}}
        <script src="{{relateToAssets}}js/{{commonjsName}}?v={{moment unixtimestamp  format ="YYYYMMDDHHmmss"}}"></script>
        {{/if}}
        {{#if jsFile}}
        <script src="{{relateToAssets}}js/{{jsFile}}?v={{moment unixtimestamp  format ="YYYYMMDDHHmmss"}}"></script>
        {{/if}}
    </body>
</html>
